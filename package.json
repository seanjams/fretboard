{
    "name": "com.musictool.fretboard",
    "displayName": "Fretboard",
    "version": "1.0.0",
    "main": "src/index.js",
    "author": "Apache Cordova Team",
    "license": "Apache-2.0",
    "description": "A sample Apache Cordova application that responds to the deviceready event.",
    "keywords": [
        "ecosystem:cordova"
    ],
    "scripts": {
        "build": "yarn run clean && webpack --mode=production --progress --display-modules",
        "watch": "webpack --mode=development --progress --display-modules --watch",
        "serve": "webpack-dev-server --mode=development --progress --open",
        "deps": "rm -rf node_modules && yarn install",
        "clean": "rm -f www/bundle.js && rm -rf www/assets",
        "serve-ios": "nodemon --watch www/bundle.js --exec \"cordova run ios\"",
        "start": "npm-run-all -c -n -l -p watch serve-ios"
    },
    "devDependencies": {
        "@types/react": "^16.9.25",
        "@types/react-dom": "^16.9.5",
        "@types/styled-components": "^5.0.1",
        "compression-webpack-plugin": "^2.0.0",
        "cordova-ios": "^6.2.0",
        "cordova-plugin-screen-orientation": "^3.0.2",
        "cordova-plugin-whitelist": "^1.3.4",
        "cordova-plugin-wkwebview-file-xhr": "^3.0.0",
        "css-loader": "^5.1.3",
        "file-loader": "^6.2.0",
        "nodemon": "^2.0.15",
        "npm-run-all": "^4.1.5",
        "prettier": "^2.2.1",
        "source-map-loader": "^0.2.4",
        "style-loader": "^2.0.0",
        "ts-loader": "^6.2.1",
        "typescript": "^4.4.3",
        "webpack": "^4.42.0",
        "webpack-bundle-analyzer": "^3.0.3",
        "webpack-cli": "^3.3.11",
        "webpack-dev-server": "^3.10.0",
        "webpack-merge": "^4.1.4"
    },
    "dependencies": {
        "@types/lodash": "^4.14.168",
        "@types/react-transition-group": "^4.4.2",
        "lodash": "^4.17.21",
        "react": "^16.13.1",
        "react-device-detect": "^1.17.0",
        "react-dom": "^16.13.1",
        "react-transition-group": "^4.4.2",
        "reset-css": "^5.0.1",
        "styled-components": "^5.0.1",
        "tone": "^14.7.77"
    },
    "cordova": {
        "plugins": {
            "cordova-plugin-whitelist": {},
            "cordova-plugin-screen-orientation": {},
            "cordova-plugin-wkwebview-file-xhr": {}
        },
        "platforms": [
            "ios"
        ]
    }
}
